        "./src/utils/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@services/*": [
        "./src/services/*"
      ],
      "@types/*": [
        "./src/types/*"
      ],
      "@pages/*": [
        "./src/pages/*"
      ],
      "@features/*": [
        "./src/features/*"
      ],
      "@config/*": [
        "./src/config/*"
      ]
    }
  }
}