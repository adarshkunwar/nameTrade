{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@ui/*": [
        "./src/components/ui/*"
      ],
      "@module/*": [
        "./src/components/module/*"
      ],
      "@layout/*": [
        "./src/components/layout/*"
